#include<stdio.h>
int main()
{
    int t,n,k,f,s;
    printf("Enter the number of test cases : ");
    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
        scanf("%d",&n);
        int arr[n];
        for(int j=0;j<n;j++)
        {
            scanf("%d",&arr[j]);
        }
        scanf("%d",&k);
        for(s=0;s<n;s++)
        {
            if(k==arr[s])
            {
                printf("Present");
                printf("%d",s+1);
                f=1;
                break;
            }
        }
        if(f==0)
        {
            printf("Not present ");
            printf("%d",s);
        }
    }
}, 
